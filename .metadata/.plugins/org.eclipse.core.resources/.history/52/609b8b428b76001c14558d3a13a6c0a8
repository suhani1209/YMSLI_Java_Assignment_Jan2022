package com.day2.q4;

import java.util.Scanner;

public class application {
	public static void main(String[] args) {
		
		Student student1=new Student(1230);
		student1.display();
		Student student2=new Student(1230, "Suhani");
		student2.display();
		Student student3=new Student(1230, "Suhani-Mittal" , 95);
		student3.display();
		student3.display(2000);
		
	}
}
